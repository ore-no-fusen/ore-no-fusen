{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ore-no-fusen",
  "version": "0.1.0",
  "identifier": "com.ore-no-fusen.app",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3002",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "俺の付箋",
        "width": 240,
        "height": 300,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "visible": false,
        "backgroundColor": "#111827",
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}